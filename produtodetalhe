package detalhes;

import java.util.Date;

public class produtodetalhe {


	public class produto {
		public Long codigoprod;
		public Float preco;
		public String tipo;

		public produto(Long _codigoprod, String _tipo, Float _preco) {
			this.codigoprod = _codigoprod;
			this.tipo = tipo;
			this.preco = preco;
		}
	}

 public class perecivel extends produto{
	 public perecivel(Long _codigoprod, String _tipo, Float _preco) {
		super(_codigoprod, _tipo, _preco);}
   public Date validade;}
 public class naoperecivel extends produto{

	public naoperecivel(Long _codigoprod, String _tipo, Float _preco) {
		super(_codigoprod, _tipo, _preco);
	}

 }
 
 
}
